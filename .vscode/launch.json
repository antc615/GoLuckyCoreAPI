{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: Uvicorn",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["GoLuckyCoreAPI.asgi:application", "--reload"],
      "django": true,
      "stopOnEntry": false,
      "justMyCode": true,
      "env": {
        "DJANGO_SETTINGS_MODULE": "GoLuckyCoreAPI.settings"
      }
    }
  ]
}
